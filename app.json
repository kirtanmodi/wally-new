{
  "expo": {
    "name": "wally",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wally_logo.png",
    "scheme": "wally",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.wally.app",
      "splash": {
        "image": "./assets/images/wally_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#7FAFF5"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wally_logo.png",
        "backgroundColor": "#7FAFF5"
      },
      "package": "com.wally.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wally_logo.png",
      "splash": {
        "image": "./assets/images/wally_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#7FAFF5"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wally_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#7FAFF5"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.494457099485-vnsuo8bt7hoatil2nq12a8qm0alro4hc"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "230c0b53-dfcc-4dcf-b8ef-b98c62781292"
      }
    },
    "owner": "kirtan19"
  }
}
